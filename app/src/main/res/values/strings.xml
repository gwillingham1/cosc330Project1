    <!--Word Definition Matching Game-->
    <string name="cultural_competence_word">Cultural Competence</string>
    <string name="cultural_competence_definition">Behaviors, attitudes and policies that come together,
        between people of differing cultures, that enables them to work together
        effectively.</string>

    <string name="numeracy_word">Numeracy</string>
    <string name="numeracy_definition">The ability to understand mathematical information
        to manage mathematical demands in a range of situations.</string>

    <string name="plain_language_word">Plain Language</string>
    <string name="plain_language_definition">Writing that is clear and to the point which takes
        less time to read and understand.</string>

    <string name="literacy_word">Literacy</string>
    <string name="literacy_definition">Is understanding written words such that you can participate
        in society, meet your goals and develop your knowledge and potential.</string>

    <string name="health_literacy_word">Health Literacy</string>
    <string name="health_literacy_definition">The degree that a person can understand basic
        health information and available services so that you can make appropriate health
        choices.</string>

    <string name="population_health_word">Population Health</string>
    <string name="population_health_definition">Moving resources from the hospital into the community
        to support all aspects of wellness (examples: disease management, support groups, timely
        treatment to avoid serious illness, home visits) by empowering people to take
        responsibility for their own wellness.</string>

